target 'ROS-Display' do

  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  pod 'ROSiOS-sensor_msgs'  # ROSiOS-<message_type>
  pod 'ROSiOS-tf'           # ROSiOS-<package_type>
  pod 'ROSiOS-nav_msgs'
  pod 'ROSiOS-rosgraph_msgs'
  pod 'ROSiOS-core'
  pod 'ROSiOS-std_msgs'
  pod 'ROSiOS-log4cxx'
  pod 'ROSiOS-boost'
  pod 'ROSiOS-geometry_msgs'
  pod 'AFNetworking'
  
  # Custom Fork'd Swift-3 Compatible Primary Flight Display pod
  # pod 'PrimaryFlightDisplay', :git => 'https://github.com/canopygroup/PrimaryFlightDisplay.git', :commit => '32841034bfc97db460739c02cf57b44cfee502fb'

end
