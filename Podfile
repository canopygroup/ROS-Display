# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'ROSiOS' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!
  pod 'ROSiOS'
  pod 'ROSiOS-sensor_msgs'  # ROSiOS-<message_type>
  pod 'ROSiOS-tf'           # ROSiOS-<package_type>
  pod 'ROSiOS-nav_msgs'
  pod 'ROSiOS-rosgraph_msgs'
  
  # Custom Fork'd Swift-3 Compatible Primary Flight Display pod
  pod 'PrimaryFlightDisplay', :git => 'https://github.com/canopygroup/PrimaryFlightDisplay.git', :commit => '6022bd391aff806907329c3d2653979fb7a00e90'

  # Pods for ROSiOS

end
